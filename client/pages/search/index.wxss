.section{
  width: 100%;
  position: fixed;
  text-align: center;
  padding: 15rpx 0 0 0;
  height: 5%;
  z-index: 100;
  font-size: 30rpx;
  background: white;
}
.dateinfo{
  padding: 22% 0 0 0;
}
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:12%;
    justify-content: center;
    text-align: left;
    font-size: 25rpx;
}
.bg-w{
  background: snow;
  margin: 7% 0 0 0;
  position: fixed;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 12%;
  color: black;
  display: flex;
  height: 3rem;
  align-items: center;
  padding: 15rpx 0 0 0; 
  font-size: 25rpx;
}
.time{
  text-align: left;
}